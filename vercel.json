{
  "version": 2,
  "rewrites": [
    { "source": "/", "destination": "/login.html" },
    { "source": "/index", "destination": "/index.html" },
    { "source": "/index.html", "destination": "/index.html" }
  ],
  "redirects": [
    { "source": "/home", "destination": "/index.html", "permanent": true },
    { "source": "/main", "destination": "/index.html", "permanent": true }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Content-Type-Options", "value": "nosniff" },
        { "key": "X-Frame-Options", "value": "DENY" },
        { "key": "X-XSS-Protection", "value": "1; mode=block" },
        { "key": "Referrer-Policy", "value": "strict-origin-when-cross-origin" }
      ]
    },
    {
      "source": "/login.html",
      "headers": [
        { "key": "Cache-Control", "value": "no-cache, no-store, must-revalidate" }
      ]
    }
  ],
  "buildCommand": null,
  "outputDirectory": null,
  "installCommand": null,
  "framework": null
}
